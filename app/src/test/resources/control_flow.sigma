int isPrime(int n) {
    if (n <= 1) {
        return 0;
    }
    int i = 2;
    while (i * i <= n) {
        if (n % i == 0) {
            return 0;
        }
        i = i + 1;
    }
    return 1;
}

int number = 17;
int primeResult = isPrime(number);
if (primeResult == 1) {
    println(number + " is prime");
} else {
    println(number + " is not prime");
}